{
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "consistent",
    "arrowParens": "always",
    "bracketSpacing": true,
    "bracketSameLine": true,
    "endOfLine": "auto",
    "importOrder": [
      "<THIRD_PARTY_MODULES>",
      "/routes/",
      "/layouts/",
      "/partials/",
      "/components/",
      "/contexts/",
      "/hooks/",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  }
  